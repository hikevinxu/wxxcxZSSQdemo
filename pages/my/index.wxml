    <view class="userElse">
      <view class="jiangbei">
        <view class="top"> <text>0</text> </view>
        <view class="bottom"> <text>奖杯</text> </view> 
      </view>
      <view class="shangjin">
        <view class="top"> <text>0.96元</text> </view>
        <view class="bottom"> <text>零钱</text> </view> 
      </view>
    </view>
    <view class="userActivity">
      <view class="duihuanBtn"> <view class="btn">兑换零钱</view> </view>
