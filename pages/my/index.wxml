  <view class="menuList">
    <view class="menuListItem">
      <image class="menuImage" src="../../images/share.png"></image>
      <button open-type="share" class="menuText">
        <view class="menuTextLeft"> 分享与保存 </view>
        <image class="menuRight" src="../../images/next.png"></image>
      </button>
    </view>
    <view class="menuListItem">
      <image class="menuImage" src="../../images/member.png"></image>
      <view class="menuText" >
        <view class="menuTextLeft"> 我的主页 </view>
        <image class="menuRight" src="../../images/next.png"></image>
      </view>
