.myFuLiCard{
  width: 100%;
  height: 100%;
  background-color: #f3f3f3;
}
.myFuLiCard .tabTitle{
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-around;
  position: fixed;
  top: 0;
  left: 0;
  background-color:#ffffff;
  color: #333;
  text-align: center;
  font-size: 14px;
}
.myFuLiCard .tabTitle .tabItem{
  display: inline-block;
  padding: 20rpx 0;
}
.myFuLiCard .tabItem.active{
  border-bottom: 1px solid #dd2219;
}
.myFuLiCard .tabContent{
  height: 500rpx;
  box-sizing: border-box;
  padding-top: 80rpx;
  background-color: #f3f3f3;
}
.myFuLiCard .swiperItem .noData{
  display: block;
  width: 300rpx;
  height: 300rpx;
  margin: 200rpx auto;
}
.scroll-h{
  height: 100%;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}